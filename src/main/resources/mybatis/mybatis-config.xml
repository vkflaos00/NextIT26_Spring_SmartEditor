<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- mybatis를 이용하여 VO에 리턴값을 받을때 카멜케이스 적용 ( bo_no -> boNo ) -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
 
 	<typeAliases>
 		<typeAlias alias="code" type="kr.or.nextit.code.vo.CodeVO"/>

 		<package name="kr.or.nextit.code.vo"/>
 		<package name="kr.or.nextit.common.vo"/>
 		<package name="kr.or.nextit.member.vo"/>
 		<package name="kr.or.nextit.free.vo"/>
 		<package name="kr.or.nextit.reply.vo"/>
 		<package name="kr.or.nextit.attach.vo"/>
 	
 	</typeAliases>
 
</configuration>